{
    "name": "your-name-here/Importer",
    "description": "Importer plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Importer\\": "plugins/Importer/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Importer\\Test\\": "plugins/Importer/tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
